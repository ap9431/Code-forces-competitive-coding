#include <iostream> 
 
using namespace std; 
 
int main() { 
    int t; 
    cin>>t; 
    while(t--){ 
        int f=0; 
        string str; 
        cin>>str; 
        string res="YES"; 
        for(int i=0;i<3;i++){ 
            if(toupper(str[i])!=res[i]){ 
                f=1; 
                cout<<"NO"<<endl; 
                break; 
            } 
        } 
        if(f==0) 
        cout<<"YES"<<endl; 
    } 
}
